apiVersion: certmanager.k8s.io/v1alpha1
kind: Certificate
metadata:
  name: k8s-multi-com-tls
spec:
  secretName: k8s-multi-com
  issuerRef:
    name: letsencrypt-prod
    kind: ClusterIssuer
  commonName: k8s-multi.com
  dnsNames:
    - k8srockzs.tk
    - www.k8srockzs.tk
  acme:
    config:
      - http01:
          ingressClass: nginx
        domains:
          - k8srockzs.tk
          - www.k8srockzs.tk
